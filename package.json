    "url": "https://github.com/iramirezc/fem-complete-intro-to-react-v5/issues"
  },
  "homepage": "https://github.com/iramirezc/fem-complete-intro-to-react-v5#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@emotion/babel-preset-css-prop": "^10.2.1",
    "@testing-library/react": "^11.2.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
